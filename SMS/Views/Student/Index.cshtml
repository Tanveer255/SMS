@model IEnumerable<SMS.Models.Data.Models.Student>

<h1 class="text-center"><i class="fas fa-comments-dollar text-success"></i> Student Historey</h1>


<table class="table">
    <thead class="thead-light">
        <tr>
            <th class="hide">
                @Html.DisplayNameFor(model => model.ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
